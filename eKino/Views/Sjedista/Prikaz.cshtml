
@{
    ViewData["Title"] = "Prikaz";

}

@model SjedistaPrikazVM
        
@for(int i=0; i<Model.Sjedista.Count; i+=10)
{
<div class="seats-row">
    @for (int j = i; j <i+10; j++)
    {
        <div class="@Model.Sjedista[j].cssClass">@Model.Sjedista[j].Red@Model.Sjedista[j].Kolona</div>
    }

</div>
}

